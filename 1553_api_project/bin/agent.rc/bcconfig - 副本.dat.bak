MinorFrameCount = 10;	/*MinorFrameCount指的是小帧个数，一个大帧里由多个小帧组成*
MinorFrameTime = 20;  /*MinorFrameTime指的是一个小帧的周期，一个小帧里由多条消息组成*/
MsgPlaceCount = 2;	 /*MsgPlaceCount指的是消息位置个数，一般由几条消息，就需要几个位置个数*/

tempFrameID = {1,1}; /*tempFrameID指的是临时帧ID，也叫帧的编号，每一条消息需要给它指明所属的帧ID*/
tempMsgID = {1,2};   /*tempMsgID指的是临时的消息ID，每一条消息都有唯一的消息ID号去加以区分，这个的
                      值的设置应该和msg_id一致*/
                       
MsgCount = 2;	       /*消息的个数，以下参数等号右边第一列的值代表第一条消息的值，第二列代表第二条消息的值，
                       以后的以此类推。比如Msg_id = {1,2}中1代表的是第一条消息的msg_id的值,2代表的是第
                       二条消息的msg_id的值。*/
Msg_id = {1,2};      /*消息的ID号，每一条消息都有唯一的ID号加以区分*/
Block_id = {1,2};    /*数据块(数据字)的ID号，每一条消息都有唯一的数据块ID号加以区分*/
Msg_type = {0,0};    /*消息的类型，目前共支持7中消息的类型,BC-TO-RT对应的值应设置为0，RT-TO-BC对应的值应设
                        为1，RT-TO-RT的对应的值应设为2，MODE-CODE(方式命令)对应的值应设为3，BC-TO-RT BROADCAST
                        对应的值应设为4，RT-TO-RT BROADCAST对应的值应设为5，MODE-CODE BROADCAST(方式命令广播消息)
                        对应的值应设为6*/
Is_async = {0,0};     /*同步(周期)或异步(非周期)消息的选择，0为同步消息，1为异步消息*/
Gap_time = {12,800};  /*消息间隔，每两条消息之间都有时间间隔*/
Msg_length = {20,0};  /*消息长度或方式命令中复用为方式码，这个值为命令字中的数据长度或命令字*/
Data = {[0x0001,0x0002,0x3,0x4,0x5]|[0x55aa,0x8899,0xbbcc]};/*数据字里填充的内容，最多32个*/
Channel_select = {0,0};  /*通道选择，由于1553B有A和B两条通道可供选择，0代表A通道，1代表B通道，2代表AB通道交替*/
Is_retry = {1,0};        /*是否重试功能，当第一次发送失败时，若设置了重试功能则可以第二次重新发送，1代表支持
                           重试功能，0代表不支持重试功能*/
Source_addr = {1,0};     /*源地址的值*/
Source_sub_addr = {2,0}; /*源子地址的值*/
Dest_addr = {3,0};       /*目的地址的值*/
Dest_sub_addr = {4,0};   /*目的子地址的值*/
Send_timing = {0,0};     /*发送时机，0代表固定发送，1代表根据别的消息内容判断是否发送_同步，2代表根据别的
                           消息内容判断是否发送_异步，3代表数据注入*/
Cyc_time = {0,0};        /*周期性发送消息的周期*/
Is_check_crc = {1,0};    /*是否支持CRC校验，0代表不支持，1代表支持*/
Crc_mode = {1,0};        /*CRC校验模式选择，0代表累加和模式，1代表CRC模式*/
Crc_timing = {0,0};      /*校验时机，0代表效验码放到消息末尾，1代表效验码放到包末尾*/
Is_from_file = {1,0};   /*是否有仿真文件，1代表有，0代表没有*/
Pack_len = {67,0};      //包长度,单位是字
Filename = {RT1_SA1_1.DAT,RT1_SA1_1.DAT}; //仿真文件文件名
